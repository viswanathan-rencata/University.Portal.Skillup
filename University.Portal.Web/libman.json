{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap-table@1.21.2",
      "destination": "wwwroot/lib/bootstrap-table/",
      "files": [
        "dist/bootstrap-table.min.css",
        "dist/bootstrap-table.min.js",
        "dist/extensions/filter-control/bootstrap-table-filter-control.min.js",
        "dist/extensions/filter-control/bootstrap-table-filter-control.min.css"
      ]
    },
    {
      "library": "bootstrap-select@1.13.18",
      "destination": "wwwroot/lib/bootstrap/bootstrap-select/",
      "files": [
        "js/bootstrap-select.min.js",
        "css/bootstrap-select.min.css"
      ]
    },
    {
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/font-awesome.min.css"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "fonts/fontawesome-webfont.eot",
        "fonts/fontawesome-webfont.svg",
        "fonts/fontawesome-webfont.ttf",
        "fonts/fontawesome-webfont.woff",
        "fonts/fontawesome-webfont.woff2",
        "fonts/FontAwesome.otf",
        "css/font-awesome.min.css"
      ]
    }
  ]
}